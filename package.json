{
  "name": "oml",
  "displayName": "OML",
  "description": "OML support for Visual Studio Code by Cynack",
  "version": "0.1.0",
  "publisher": "Cynack",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "http://github.com/cynack/vscode-oml.git"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "oml",
        "aliases": [
          "OML",
          "oml"
        ],
        "extensions": [
          "oml"
        ],
        "configuration": "./language/oml-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "oml",
        "scopeName": "source.oml",
        "path": "./syntaxes/oml-tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "oml",
        "path": "./snippets/oml-snippets.json"
      }
    ]
  }
}
